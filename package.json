{
  "name": "pruebas",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "wrangler pages dev ./dist",
    "deploy": "wrangler pages deploy ./dist",
    "astro": "astro",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:html": "vitest run --coverage && start coverage/index.html",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:auth": "playwright test auth.spec.ts",
    "test:e2e:calendar": "playwright test calendar.spec.ts",
    "test:e2e:firefox": "playwright test --project=public-firefox",
    "test:e2e:safari": "playwright test --project=public-webkit",
    "test:e2e:mobile": "playwright test --project=public-mobile-chrome --project=public-mobile-safari",
    "test:e2e:all-browsers": "playwright test --project=public --project=public-firefox --project=public-webkit",
    "test:all": "vitest run && playwright test --project=public",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/react": "^4.0.0",
    "@astrojs/rss": "^4.0.14",
    "@astrojs/sitemap": "^3.6.0",
    "@astrojs/tailwind": "^6.0.0",
    "@googleapis/calendar": "^14.2.0",
    "@googleapis/docs": "^9.2.0",
    "@googleapis/drive": "^20.0.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.90.0",
    "astro": "^5.0.0",
    "google-auth-library": "^10.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "resend": "^6.7.0",
    "stripe": "^20.1.2",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@astrojs/cloudflare": "^12.6.12",
    "@astrojs/vercel": "^9.0.4",
    "@axe-core/playwright": "^4.11.0",
    "@eslint/js": "^9.17.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitest/coverage-v8": "^4.0.17",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.2.3",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "jsdom": "^27.4.0",
    "msw": "^2.12.7",
    "playwright": "^1.57.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.21.0",
    "typescript-eslint": "^8.53.0",
    "vitest": "^4.0.17",
    "wrangler": "^4.59.3"
  }
}
